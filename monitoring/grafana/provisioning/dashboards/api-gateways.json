{
    "dashboard": {
      "id": null,
      "uid": "api-gateway-benchmark",
      "title": "API Gateway Benchmarking",
      "timezone": "browser",
      "panels": [
        {
          "title": "API Gateway Latency (ms)",
          "type": "timeseries",
          "targets": [
            {
              "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[1m]))",
              "legendFormat": "{{instance}}",
              "refId": "A"
            }
          ]
        },
        {
          "title": "API Gateway Requests Per Second (RPS)",
          "type": "timeseries",
          "targets": [
            {
              "expr": "rate(http_requests_total[1m])",
              "legendFormat": "{{instance}}",
              "refId": "B"
            }
          ]
        },
        {
          "title": "API Gateway CPU Usage (%)",
          "type": "timeseries",
          "targets": [
            {
              "expr": "rate(container_cpu_usage_seconds_total[1m]) * 100",
              "legendFormat": "{{instance}}",
              "refId": "C"
            }
          ]
        },
        {
          "title": "API Gateway Memory Usage (MB)",
          "type": "timeseries",
          "targets": [
            {
              "expr": "container_memory_usage_bytes / 1024 / 1024",
              "legendFormat": "{{instance}}",
              "refId": "D"
            }
          ]
        },
        {
          "title": "API Gateway Error Rate (%)",
          "type": "timeseries",
          "targets": [
            {
              "expr": "(rate(http_requests_total{status_code=~'5..'}[1m]) / rate(http_requests_total[1m])) * 100",
              "legendFormat": "{{instance}}",
              "refId": "E"
            }
          ]
        }
      ]
    },
    "overwrite": true
  }
  